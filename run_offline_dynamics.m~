offline_force_matrix = evalin('base', 'offline_force_matrix');
offline_dt           = evalin('base','offline_dt');
offline_dt           = evalin('base','offline_dt');
X  = evalin('base', 'X_offline');
Y  = evalin('base', 'Y_offline');

F = [1 offline_dt; 0 1];

for i = 1 : 1 : n
  X_vector = [X(i); Xdot(i)];
  Y_vector = [Y(i); Ydot(i)];  

  X_vector = F * X_vector_old + B * Xdotdot(i);
  Y_vector = F * Y_vector_old + B * Ydotdot(i);
end